{
  "name": "threejs-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:gh": "tsc && vite build --base=/threejs-app-sandbox",
